{
  "name": "observer-server",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "node ./server.js",
    "dev": "cross-env NODE_ENV=development nodemon ./server.js",
    "build": "pkg . --out-path dist"
  },
  "bin": "server.js",
  "pkg": {
    "scripts": [
      "routes/**/*.js",
      "db/**/*.js",
      "utils/**/*.js",
      "worker/**/*.js",
      "middleware/**/*.js"
    ],
    "assets": [
      ".env",
      "config.js",
      "public/**/*",
      "logs/**/*",
      "output/**/*",
      "node_modules/axios/dist/**/*",
      "node_modules/pg/lib/**/*.js",
      "node_modules/pg-pool/**/*",
      "node_modules/pg/package.json"
    ],
    "dependencies": [
      "mysql2",
      "bluebird",
      "dotenv"
    ],
    "targets": [
      "node18-win-x64"
    ],
    "outputPath": "dist"
  },
  "dependencies": {
    "@mhoc/axios-digest-auth": "^0.8.0",
    "axios": "^1.13.2",
    "axios-retry": "^4.5.0",
    "bluebird": "^3.7.2",
    "child_process": "^1.0.2",
    "connect-pg-simple": "^10.0.0",
    "cookie-parser": "~1.4.7",
    "cors": "^2.8.5",
    "cross-env": "^10.0.0",
    "date-fns": "^4.1.0",
    "dayjs": "^1.11.13",
    "debug": "~4.3.7",
    "digest-fetch": "^2.0.3",
    "dotenv": "^16.4.5",
    "exceljs": "^4.4.0",
    "exec": "^0.2.1",
    "express": "~4.21.1",
    "express-session": "^1.18.1",
    "express-validator": "^7.2.1",
    "fluent-ffmpeg": "^2.1.3",
    "fs": "^0.0.1-security",
    "http-errors": "^2.0.0",
    "iconv-lite": "^0.6.3",
    "ip": "^2.0.1",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "memorystore": "^1.6.7",
    "modbus-serial": "^8.0.21-no-serial-port",
    "morgan": "~1.9.1",
    "mssql": "^11.0.1",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.14.0",
    "node-cron": "^3.0.3",
    "p-limit": "^6.2.0",
    "pdfkit": "^0.17.2",
    "pg": "^8.13.1",
    "ping": "^0.4.4",
    "playwright": "^1.55.1",
    "puppeteer": "^24.22.3",
    "request-ip": "^3.3.0",
    "rtsp-ffmpeg": "^0.0.19",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "uuid": "^11.0.5",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.15.0",
    "eslint": "^9.15.0",
    "globals": "^15.12.0",
    "nodemon": "^3.1.10",
    "pkg": "^5.8.1"
  }
}
